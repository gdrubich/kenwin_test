<!DOCTYPE html>
<html lang="en">
<head>
	<title>Kenwin Test</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
<!--===============================================================================================-->
	<link rel="icon" type="image/png" href="{{request.static_url('kenwin:static/auth_theme/images/icons/favicon.ico')}}"/>
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/vendor/bootstrap/css/bootstrap.min.css')}}">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/fonts/font-awesome-4.7.0/css/font-awesome.min.css')}}">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/fonts/iconic/css/material-design-iconic-font.min.css')}}">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/vendor/animate/animate.css')}}">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/vendor/css-hamburgers/hamburgers.min.css')}}">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/vendor/animsition/css/animsition.min.css')}}">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/vendor/select2/select2.min.css')}}">
<!--===============================================================================================-->	
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/vendor/daterangepicker/daterangepicker.css')}}">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/css/util.css')}}">
	<link rel="stylesheet" type="text/css" href="{{request.static_url('kenwin:static/auth_theme/css/main.css')}}">
<!--===============================================================================================-->
</head>
<body>
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<form class="login100-form validate-form" action="{{request.route_url('auth',action='in')}}" method="post">
					{% if request.authenticated_userid %}
						<span class="login100-form-title p-b-26">
							Welcome <strong>{{request.authenticated_userid}}</strong>
						</span>
						<div class="container-login100-form-btn">
							<div class="wrap-login100-form-btn">
								<div class="form-group login100-form-bgbtn"></div>
								<button class="login100-form-btn" value="Sign out">
									<a class="login100-form-btn" href="{{request.route_url('auth',action='out')}}">Sign out</a>
								</button>
							</div>
						</div>
					{% else %}
						<span class="login100-form-title p-b-48">
							<i class="zmdi zmdi-font"></i>
						</span>

						<div class="form-group wrap-input100 validate-input">
			                <input type="text" name="username" class="input100">
			                <span class="focus-input100" data-placeholder="Username">
			            </div>

						<div class="form-group wrap-input100 validate-input" data-validate="Enter password">
							<span class="btn-show-pass">
								<i class="zmdi zmdi-eye"></i>
							</span>
							<input class="input100" type="password" name="password">
							<span class="focus-input100" data-placeholder="Password"></span>
						</div>
						<div class="container-login100-form-btn">
							<div class="wrap-login100-form-btn">
								<div class="form-group login100-form-bgbtn"></div>
								<button type="submit" value="Sign in" class="login100-form-btn">
									Login
								</button>
							</div>
						</div>
					{% endif %}
				</form>
			</div>
		</div>
	</div>
	

	<div id="dropDownSelect1"></div>
	
<!--===============================================================================================-->
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/jquery/jquery-3.2.1.min.js')}}"></script>
<!--===============================================================================================-->
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/animsition/js/animsition.min.js')}}"></script>
<!--===============================================================================================-->
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/bootstrap/js/popper.js')}}"></script>
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/bootstrap/js/bootstrap.min.js')}}"></script>
<!--===============================================================================================-->
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/select2/select2.min.js')}}"></script>
<!--===============================================================================================-->
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/daterangepicker/moment.min.js')}}"></script>
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/daterangepicker/daterangepicker.js')}}"></script>
<!--===============================================================================================-->
	<script src="{{request.static_url('kenwin:static/auth_theme/vendor/countdowntime/countdowntime.js')}}"></script>
<!--===============================================================================================-->
	<script src="{{request.static_url('kenwin:static/auth_theme/js/main.js')}}"></script>

</body>
</html>